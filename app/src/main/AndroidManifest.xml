<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jc770797.catimageprocess">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:debuggable="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
        tools:ignore="HardcodedDebugMode">

        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ImageSelectionActivity"
            android:screenOrientation="portrait"
            android:label="Image Selection"/>
        <activity
            android:name=".ImageCropActivity"
            android:screenOrientation="portrait"
            android:label="Image Crop"/>
        <activity
            android:name=".ImageEditingActivity"
            android:screenOrientation="portrait"
            android:label="Basic Image Editing"/>
        <activity
            android:name=".ImageMorphological"
            android:screenOrientation="portrait"
            android:label="Morphological Editing"/>
        <activity
            android:name=".ImageSnakeActivity"
            android:screenOrientation="portrait"
            android:label="Snake"/>
        <activity
            android:name=".SnakeOverlayActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Translucent"
            android:label="Snake"/>
        <activity android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat"/>
    </application>


</manifest>